{
  "name": "demo-dapp",
  "description": "Ethereum Demo Dapp built on Sequence stack",
  "version": "0.1.0",
  "private": true,
  "homepage": "demo-dapp",
  "scripts": {
    "dev": "BROWSER=none pnpm start",
    "start": "vite",
    "build": "BUILD_PATH='./dist' tsc && vite build",
    "typecheck": "tsc --noEmit",
    "serve": "vite preview",
    "dist": "pnpm build",
    "link-sequence": "pnpm run clear:vite:cache && ../sequence.js/scripts/pnpm-link.sh link",
    "unlink-sequence": "pnpm run clear:vite:cache && ../sequence.js/scripts/pnpm-link.sh unlink",
    "clear:vite:cache": "rm -rf node_modules/.vite/"
  },
  "dependencies": {
    "0xsequence": "2.2.13",
    "@0xsequence/abi": "2.3.23",
    "@0xsequence/design-system": "^3.0.1",
    "@0xsequence/ethauth": "^1.0.0",
    "@0xsequence/network": "2.3.23",
    "@0xsequence/provider": "2.3.29",
    "@0xsequence/utils": "2.3.23",
    "@tanstack/react-query": "^5.90.16",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.2.2",
    "@vanilla-extract/css": "^1.17.4",
    "ethers": "^6.13.4",
    "framer-motion": "^12.23.15",
    "react": "^19.0.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.9.2",
    "viem": "^2.44.1",
    "wagmi": "0.0.0-canary-20260110074453"
  },
  "devDependencies": {
    "@vanilla-extract/vite-plugin": "^5.0.2",
    "@vitejs/plugin-react": "^5.0.0",
    "vite": "^7.1.3",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
